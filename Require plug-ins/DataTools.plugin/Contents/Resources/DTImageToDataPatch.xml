			<array>
				<string>TIFF</string>
				<string>BMP</string>
				<string>GIF</string>
				<string>JPEG</string>
				<string>PNG</string>
				<string>JPEG2000</string>
			</array>
		</dict>
	</dict>
	<key>outputAttributes</key>
	<dict>
		<key>outputRawData</key>
		<dict>
			<key>name</key>
			<string>Raw Data</string>
